<form method="post" action="{{ path('program_session_delete', {'id': program_session.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ program_session.id) }}">
    <button class="btn"><span class="icon-bin"></span> Delete</button>
</form>
